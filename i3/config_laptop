	bindsym $mod+minus exec ~/urxvt_shell.sh
	bindsym $mod+Shift+minus exec ~/urxvt_shell_dup.sh
	# bindsym $mod+minus exec urxvt -fn "xft:Hack:pixelsize=14,xft:IPAGothic:pixelsize=14" -name terminal
	# bindsym $mod+Shift+minus exec urxvt -fn "xft:Hack:pixelsize=14,xft:IPAGothic:pixelsize=14" -name tmux
	
	# kill focused window
	bindsym $mod+Shift+w kill
	
	# start dmenu (a program launcher)
	bindsym $mod+d exec dmenu_run -nb black -sb blue -l 8 -fn 'Hack 5'
	# There also is the (new) i3-dmenu-desktop which only displays applications
	# shipping a .desktop file. It is a wrapper around dmenu, so you need that
	# installed.
	# bindsym $mod+d exec --no-startup-id i3-dmenu-desktop
	
	# change focus
	bindsym $mod+h focus left exec $autokey
	bindsym $mod+t focus down exec $autokey
	bindsym $mod+n focus up exec $autokey
	bindsym $mod+s focus right exec $autokey
	
	# move focused window
	bindsym $mod+Shift+h move left
	bindsym $mod+Shift+t move down
	bindsym $mod+Shift+n move up
	bindsym $mod+Shift+s move right
	
	# alternatively, you can use the cursor keys:
	bindsym $mod+Shift+Left move left
	bindsym $mod+Shift+Down move down
	bindsym $mod+Shift+Up move up
	bindsym $mod+Shift+Right move right
	
	# split in horizontal orientation
	bindsym $mod+z split h
	
	# split in vertical orientation
	bindsym $mod+v split v
	
	# enter fullscreen mode for the focused container
	bindsym $mod+e fullscreen toggle
	
	# change container layout (stacked, tabbed, toggle split)
	# bindsym $mod+o layout stacking
	bindsym $mod+Shift+a layout tabbed
	bindsym $mod+Shift+o layout toggle split
	# Define names for default workspaces for which we configure key bindings later on.
	# We use variables to avoid repeating the names in multiple places.
	set $ws61 "61:6 壱"
	set $ws62 "62:6 弐"
	set $ws63 "63:6 参"
	set $ws64 "64:6 肆"
	set $ws65 "65:6 伍"
	set $ws66 "66:6 陸"
	set $ws67 "67:6 漆"
	set $ws68 "68:6 捌"
	set $ws69 "69:6 玖"
	set $ws70 "70:6 拾"
	
	# switch to workspace
	bindsym $mod+1 workspace $ws61 exec $autokey
	bindsym $mod+2 workspace $ws62 exec $autokey
	bindsym $mod+3 workspace $ws63 exec $autokey
	bindsym $mod+4 workspace $ws64 exec $autokey
	bindsym $mod+5 workspace $ws65 exec $autokey
	bindsym $mod+6 workspace $ws66 exec $autokey
	bindsym $mod+7 workspace $ws67 exec $autokey
	bindsym $mod+8 workspace $ws68 exec $autokey
	bindsym $mod+9 workspace $ws69 exec $autokey
	bindsym $mod+0 workspace $ws70 exec $autokey
	
	# move focused container to workspace
	bindsym $mod+Shift+1 move container to workspace $ws61
	bindsym $mod+Shift+2 move container to workspace $ws62
	bindsym $mod+Shift+3 move container to workspace $ws63
	bindsym $mod+Shift+4 move container to workspace $ws64
	bindsym $mod+Shift+5 move container to workspace $ws65
	bindsym $mod+Shift+6 move container to workspace $ws66
	bindsym $mod+Shift+7 move container to workspace $ws67
	bindsym $mod+Shift+8 move container to workspace $ws68
	bindsym $mod+Shift+9 move container to workspace $ws69
	bindsym $mod+Shift+0 move container to workspace $ws70

	# back to normal: Enter or Escape or $mod+r
	bindsym Return mode "default"
	bindsym $mod+Shift+k mode "default"
	bindsym $mod+r mode "resize"
	bindsym $mod+j mode "一"
	bindsym $mod+k mode "二"
	bindsym $mod+Shift+j mode "三"
	bindsym $mod+Shift+k mode "四"
	bindsym $mod+b mode "五"
	bindsym $mod+m mode "default"
}


# Sreen brightness controls
bindsym XF86MonBrightnessUp exec xbacklight -inc 10 # increase screen brightness
bindsym XF86MonBrightnessDown exec xbacklight -dec 10 # decrease screen brightness

# change volume or toggle mute
bindsym XF86AudioRaiseVolume exec amixer -q -D pulse sset Master 5%+ && pkill -RTMIN+1 i3blocks 
bindsym XF86AudioLowerVolume exec amixer -q -D pulse sset Master 5%- && pkill -RTMIN+1 i3blocks
bindsym XF86AudioMute exec amixer -q -D pulse sset Master toggle && pkill -RTMIN+1 i3blocks
# bindsym XF86AudoMute exec amixer set Master toggle
# bindsym XF86AudioLowerVolume exec amixer set Master 5-
# bindsym XF86AudioRaiseVolume exec amixer set Master 5+

# reload the configuration file
bindsym $mod+Shift+c reload
# restart i3 inplace (preserves your layout/session, can be used to upgrade i3)
bindsym $mod+Shift+r restart
# exit i3 (logs you out of your X session)
bindsym $mod+Shift+g exec "i3-nagbar -t warning -m 'You pressed the exit shortcut. Do you really want to exit i3? This will end your X session.' -B 'Yes, exit i3' 'i3-msg exit'"
# i3lock
bindsym $mod+Shift+l exec ~/.config/i3/i3lock
# bindsym $mod+Shift+l exec i3lock --color "$base02"

# mouse focus
focus_follows_mouse no

bindsym $mod+r mode "resize"
# resize window (you can also use the mouse for that)
mode "resize" {
	# These bindings trigger as soon as you enter the resize mode
	bindsym s resize shrink width 10 px or 10 ppt
	bindsym n resize grow height 10 px or 10 ppt
	bindsym t resize shrink height 10 px or 10 ppt
	bindsym h resize grow width 10 px or 10 ppt
	
	# same bindings, but for the arrow keys
	bindsym Left resize shrink width 10 px or 10 ppt
	bindsym Down resize grow height 10 px or 10 ppt
	bindsym Up resize shrink height 10 px or 10 ppt
	bindsym Right resize grow width 10 px or 10 ppt
	
	# back to normal: Enter or Escape or $mod+r
	# bindsym Return mode "default"
	# bindsym $mod+r mode "default"
	# bindsym $mod+minus mode "default"
	bindsym $mod+j mode "一"
	bindsym $mod+k mode "二"
	bindsym $mod+Shift+j mode "三"
	bindsym $mod+Shift+k mode "四"
	bindsym $mod+b mode "五"
	bindsym $mod+m mode "六"
}

